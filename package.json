{
  "name": "@sudoplatform/sudo-site-reputation",
  "version": "0.0.0",
  "author": "Anonyome Labs, Inc.",
  "repository": "git@gitlab.tools.anonyome.com:platform/site-reputation/site-reputation-js.git",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=20 <21"
  },
  "scripts": {
    "lint": "yarn lint:eslint && yarn lint:prettier",
    "lint:eslint": "eslint \"{bin,integration-tests,src}/**/*.{ts,tsx}\"",
    "lint:prettier": "prettier -l \"{bin,integration-tests,src}/**/*.{js,json,jsx,ts,tsx}\"",
    "codegen": "graphql-codegen -c codegen.yml",
    "test": "npm-run-all --parallel test:unit test:system",
    "test:unit": "jest",
    "test:system": "jest --runInBand -c jest.system.config.json",
    "build": "yarn codegen && npm-run-all --parallel build:clean build:declarations build:docs build:transpile:cjs build:transpile:esm",
    "build:clean": "rm -rf lib",
    "build:declarations": "tsc --project tsconfig.declarations.json",
    "build:docs": "typedoc --excludePrivate --out docs src/index.ts",
    "build:transpile:cjs": "tsc --project tsconfig.commonjs.json",
    "build:transpile:esm": "tsc",
    "verify": "./node_modules/.bin/audit-with-suppressions && yarn lint && yarn test"
  },
  "main": "./cjs/index.js",
  "types": "./types/index.d.ts",
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "import": "./lib/index.js",
      "require": "./cjs/index.js",
      "default": "./lib/index.js"
    }
  },
  "files": [
    "cjs",
    "docs",
    "lib",
    "types"
  ],
  "sideEffects": false,
  "resolutions": {
    "apollo-cache-inmemory": "^1.6.6",
    "json5": "^2.2.3",
    "lodash-es": "^4.17.21",
    "node-fetch": "^2.7.0",
    "undici": "^7.15.0",
    "yaml": "^2.8.1",
    "fast-xml-parser": "^5.2.5",
    "semver": "^7.7.2"
  },
  "peerDependencies": {
    "@sudoplatform/sudo-api-client": "^15.0.0",
    "@sudoplatform/sudo-common": "^10.0.2",
    "@sudoplatform/sudo-user": "^15.1.1"
  },
  "dependencies": {
    "apollo-cache-inmemory": "^1.6.6",
    "aws-appsync": "4.1.10",
    "aws-cdk": "^2.1029.0",
    "fake-indexeddb": "^6.2.2",
    "fp-ts": "^2.16.11",
    "graphql": "^15.10.1",
    "io-ts": "^2.2.22",
    "lodash": "^4.17.21",
    "tslib": "^2.8.1",
    "url-parse": "^1.5.10"
  },
  "devDependencies": {
    "@aws-sdk/client-cognito-identity-provider": "^3.552.0",
    "@babel/cli": "^7.28.3",
    "@babel/core": "^7.28.4",
    "@babel/helper-hoist-variables": "^7.24.7",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.18.6",
    "@babel/plugin-proposal-numeric-separator": "^7.18.6",
    "@babel/plugin-proposal-optional-chaining": "^7.21.0",
    "@babel/plugin-proposal-private-methods": "^7.18.6",
    "@babel/plugin-syntax-import-meta": "^7.10.4",
    "@babel/plugin-transform-modules-commonjs": "^7.27.1",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-typescript": "^7.27.1",
    "@graphql-codegen/add": "^6.0.0",
    "@graphql-codegen/cli": "^6.0.0",
    "@graphql-codegen/typed-document-node": "^6.0.0",
    "@graphql-codegen/typescript": "^5.0.0",
    "@graphql-codegen/typescript-operations": "^5.0.0",
    "@sudoplatform/sudo-api-client": "^15.0.0",
    "@sudoplatform/sudo-common": "^11.0.0",
    "@sudoplatform/sudo-entitlements": "^17.0.0",
    "@sudoplatform/sudo-entitlements-admin": "^12.1.0",
    "@sudoplatform/sudo-user": "^15.1.1",
    "@types/bunyan": "^1.8.11",
    "@types/jest": "^30.0.0",
    "@types/lodash": "^4.17.20",
    "@types/node": "^20.19.13",
    "@types/url-parse": "^1.4.11",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "babel-plugin-bundled-import-meta": "^0.3.2",
    "bunyan": "^1.8.15",
    "dotenv": "^17.2.2",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-tree-shaking": "^1.12.2",
    "import-sort-style-module": "^6.0.0",
    "isomorphic-fetch": "^3.0.0",
    "isomorphic-webcrypto": "^2.3.8",
    "jest": "^30.1.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.6.2",
    "prettier-eslint": "^16.4.2",
    "prettier-plugin-import-sort": "^0.0.7",
    "ts-jest": "^29.4.1",
    "typedoc": "^0.28.12",
    "typescript": "^5.9.2"
  },
  "importSort": {
    ".js, .jsx, .ts, .tsx": {
      "style": "module",
      "parser": "typescript"
    }
  },
  "auditSuppressions": {},
  "packageManager": "yarn@4.9.4+sha512.7b1cb0b62abba6a537b3a2ce00811a843bea02bcf53138581a6ae5b1bf563f734872bd47de49ce32a9ca9dcaff995aa789577ffb16811da7c603dcf69e73750b"
}
